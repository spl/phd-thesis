%format ^^              = "{}"
%format ^^<             = "\!{}"
%format ^^>             = "\,{}"

%format small           = "\,{}"
%format medium          = "\:{}"
%format large           = "\;{}"
%format quad            = "\quad{}"
%format qquad           = "\qquad{}"

% Around ::= in syntax definitions
%format synspace        = medium
