%format (sup(f)(x))             = "{" f "}^{" x "}"
%format (sup'(f)(x))            = "{" f "}\,^{" x "}"
%format (sub(f)(x))             = "{" f "}_{" x "}"

%format (p_app(f)(x))           = "{" f "}(" x ")"
%format (p_app'(f)(x))          = "{" f "}\,(" x ")"
%format (a_app(f)(x))           = "{" f "}\langle{}" x "\rangle{}"

%format (subst_one(x)(e)(f))    = "[" x +-> e "]" f

%format (magnitude(x))          = "|" x "|"
%format (arity(g)(x))           = "|" x "|_{" g "}"
